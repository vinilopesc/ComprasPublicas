# Exemplo de arquivo .env para configurações da aplicação
# Copie este arquivo para .env e faça os ajustes necessários

# URL base da API do TCE-MG
TCE_API_BASE_URL=https://bancodepreco.tce.mg.gov.br/api/public

# Configurações de cache
CACHE_ENABLED=true
CACHE_EXPIRATION=3600

# Nível de log (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Diretório para armazenar arquivos exportados
EXPORT_DIR=exports 